      "source": [
        "from sympy import *\n",
        "\n",
        "a3, x, l = var(\"a3, x, l\")\n",
        "\n",
        "w = a3*x*x*(x - l)\n",
        "\n",
        "tmp = integrate(w, (x,0,l))\n",
        "pprint(tmp)\n"
      ],
